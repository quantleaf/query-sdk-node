{
  "name": "@quantleaf/query-sdk-node",
  "version": "1.0.24",
  "description": "Quantleaf Query SDK for Node",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "publish-npm": "npm --userconfig=./.npmrc-npm publish dist --access public",
    "publish-git": "npm --registry=https://npm.pkg.github.com/quantleaf --userconfig=./.npmrc-git publish dist",
    "publish-all": "npm run publish-npm && npm run publish-git",
    "build-publish": "npm run prepare-publish && npm run publish-all",
    "prepare-publish": "rimraf dist && tsc --project tsconfig.prod.json && ncp package.json dist/package.json",
    "patch-publish": "npm update && git add . && git commit -m \"Update dependencies\" && git push origin master && npm version patch && npm run build-publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quantleaf/query-sdk-node.git"
  },
  "author": "quantleaf",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^8.10.66",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.2.1",
    "ncp": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rewire": "^5.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@quantleaf/code-language": "^1.0.9",
    "@quantleaf/query-request": "^1.0.11",
    "@quantleaf/query-result": "^1.0.35",
    "@quantleaf/query-schema": "^1.0.43",
    "axios": "^0.21.1"
  },
  "readme": "README.md"
}
